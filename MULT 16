	ADR		R0, numbers
		ADR		R7, result
		ADR		R8, carry
		LDR		R1,[R0]
		LDR		R2,[R0,#4]
		MOV		R5, #16
		MOV		R3, #1
		MOV		R6, #0
		
loop
		AND		R4,R3,R2
		SUBS		R4,R4,R3
		ADDPL	R6,R6,R1
		LSR		R2,R2,#1
		LSL 		R1,R1,#1
		SUBS 	R5,R5,#1
		BNE		loop
		STR 		R6, [R7]
		end
		
numbers	DCD		0b1100101010010001, 0b1111010101000011
result	FILL		4
carry	FILL		4
